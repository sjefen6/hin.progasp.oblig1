@model IEnumerable<MVC_Oblig1.Models.Channel>

@{
    ViewBag.Title = "Channels";
}

<h2>Channels</h2>

<table>
    <tr>
        <th>
            Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Join", "Join", new { id = item.Name })
            @Html.ActionLink("Display", "Display", new { id = item.Name })
        </td>
    </tr>
}

</table>
