@model IEnumerable<MVC_Oblig1.Models.Permission>

@{
    ViewBag.Title = "Channels";
}

<h2>Channels</h2>

<p>
    @Html.ActionLink("Join", "Join")
</p>
<table>
    <tr>
        <th>
            Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ChannelName)
        </td>
        <td>
            @Html.ActionLink("Display", "Display", new { id = item.ChannelName })
        </td>
    </tr>
}

</table>

<div>
    @Html.ActionLink("All Channels", "All")
</div>